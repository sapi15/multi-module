<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="defaultExecutorType" value="REUSE"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.multi.module.member.entity.UserDto"                    alias="userDto" />
        <typeAlias type="com.multi.module.history.entity.HistoryDto"                alias="historyDto" />
        <typeAlias type="com.multi.module.board.entity.BoardDto"                    alias="boardDto" />
        <typeAlias type="com.multi.module.attachFile.entity.AttachFileDto"          alias="attachFileDto" />

    </typeAliases>
</configuration>
